<h1 class="display-5 fw-bold text-body-emphasis text-center">Hello JavaScript</h1>

<div class="card bg-dark text-white">
  <img class="card-img" src="https://storage.kun.uz/source/6/apKC72vHGkCger4VdHK6a9Rg8u7Aej8Y.jpg" alt="Card image">
  <div class="card-img-overlay">
    <h5 class="card-title">Card title</h5>
    <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
    <p class="card-text">Last updated 3 mins ago</p>
  </div>
</div>


{{#if kampaniya.length}}
{{#each kampaniya}}
<div class="col-lg-3 col-md-6 mb-4 mb-lg-0">
      <!-- Card-->
      <div class="card rounded shadow-sm border-0">
        <div class="card-body p-4">
            <img src="{{img}}" alt="{{title}}">
          <h5 class="name"><a href="#" class="text-dark">{{name}}</a></h5>
          <p class="vazifasi">{{vazifasi}}</p>
          <b><p class="maosh">{{maosh}}</p></b>
        </div>
      </div>
    </div>
{{/each}}
{{else}}
    <p>Hozircha hech qanday e`lon yoq</p>
{{/if}}

